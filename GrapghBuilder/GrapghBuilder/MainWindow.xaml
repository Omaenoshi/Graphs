<Window x:Class="GrapghBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GrapghBuilder"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">
    <Grid>
        <Canvas x:Name="main" Background="Transparent" MouseRightButtonDown="CreateTop"
                 MouseLeftButtonDown="CreateEdge" MouseLeftButtonUp="EndEdge"></Canvas>
    </Grid>
</Window>
